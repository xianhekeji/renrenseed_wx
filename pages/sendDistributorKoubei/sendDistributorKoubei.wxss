/* pages/sendDistributorKoubei/sendDistributorKoubei.wxss */

.star-image {
  position: absolute;
  top: 50rpx;
  width: 100rpx;
  height: 100rpx;
  src: "../../images/normal.png";
}

.item {
  position: absolute;
  top: 50rpx;
  width: 75rpx;
  height: 150rpx;
}

.input_content {
  min-height: 200rpx;
}

.content {
  display: flex;
  flex-direction: column;
}

.item_img_view {
  display: flex;
  flex-direction: row;
  padding-left: 5rpx;
  text-align: center;
  justify-content: center;
  margin-top: 10rpx;
  flex-wrap: wrap;
}

.item_img {
  min-height: 200rpx;
  width: 200rpx;
  max-height: 200rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  border-radius: 10px;
  padding-top: 0rpx;
}

.addImage {
  width: 200rpx;
  height: 200rpx;
}

.item_star {
  display: flex;
  flex-direction: row;
}

.star_text {
  margin-top: 40rpx;
  margin-left:500rpx;
  color: red;
}
